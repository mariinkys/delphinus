{
    "rust-analyzer.showUnlinkedFileNotification": false,
    "emmet.includeLanguages": {
        "rust": "html",
        "*.rs": "html"
    },
    "tailwindCSS.includeLanguages": {
        "rust": "html",
        "*.rs": "html"
    },
    "files.associations": {
        "*.rs": "rust"
    },
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": true
    },
    "css.validate": false,
    "rust-analyzer.check.command": "check",
    "rust-analyzer.cargo.features": [
        "ssr"
    ],
}